<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>Securities</title>
    <link rel="stylesheet" href="/static/css/global.css" />
</head>
<body class="{% block bodyClass %}{% endblock %}">
{% block header %}
<div class="app">
    <header class="header">
        <div class="page-limit">
            <div class="header__content">
            <div class="header__logo"><a href="{{ path('home') }}">Securities</a></div>
            <div id="search-container" class="header__search">{%
                if showSearchInHead %}{% include '@partials/search.html.twig' %}{% endif
            %}</div>
            </div>
        </div>
    </header>
    <main class="main">
        <div class="page-limit" id="main-body">
{% endblock %}
{% block body %}{% endblock %}
{% block footer %}
        </div>
    </main>
</div>
{% endblock %}
<div class="hisdden">
    <div id="loading-spinner" class="loading"><div class="loading__spinner"></div></div>
</div>
<script src="/static/js/bootstrap.js"></script>
</body>
</html>
