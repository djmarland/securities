    {#<meta name="msapplication-square310x310logo" content="/static/icons/mstile-310x310.png" />#}
    {#<meta name="theme-color" content="#383b3f" />#}
    <link rel="stylesheet" href="{{ 'global.css'|asset_path }}" />
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script>
        google.charts.load('current', {'packages':['corechart']});
    </script>
<body class="{% block bodyClass %}{% endblock %}">
{% block header %}
<div class="app">
    <header class="header">
        <div class="page-limit">
            <div id="masthead-content" class="header__content">
            <div class="header__logo">
                <a href="{{ path('home') }}">{{ meta.siteTitle }}</a>
            </div>
            <div id="search-container" class="header__search">{% include '@partials/search.html.twig' %}</div>
            </div>
        </div>
    </header>
    <nav class="nav">
        <ul class="nav__list page-limit"
            ><li class="nav__item"><a class="nav__link {% if currentSection == 'securities' %}nav__link--active{% endif %}" href="{{ path('security_list') }}">Securities</a></li
            ><li class="nav__item"><a class="nav__link {% if currentSection == 'issuers' %}nav__link--active{% endif %}" href="{{ path('issuer_list') }}">Issuers</a></li
            >{#<li class="nav__item"><a class="nav__link {% if currentSection == 'industries' %}nav__link--active{% endif %}" href="{{ path('industry_list') }}">Industries</a></li
            ><li class="nav__item"><a class="nav__link {% if currentSection == 'about' %}nav__link--active{% endif %}" href="{{ path('about') }}">About</a></li
        >#}</ul>
    </nav>
    <main class="main">
        <div class="page-limit" id="main-body">
        <p class="text--center g-unit island" style="background: #d30b0a;color:white;">
            This site is in Alpha (under development). Do not use it for accurate data yet
        </p>
{% endblock %}
{% block body %}{% endblock %}
{% block footer %}
        </div>
    </main>
    <footer class="footer">
        <div class="page-limit">
            &copy; {{ currentYear }} ISIN Analytics Ltd | <a href="{{ path('terms') }}">Terms of use</a>
        </div>
    </footer>
</div>
{% endblock %}
<template id="loading-spinner" class="hidden">
    <div class="loading">
        <svg class="loading__spinner" viewBox="-2 -2 70 70" xmlns="http://www.w3.org/2000/svg">
            <circle class="loading__path" fill="none" stroke-width="8" cx="33" cy="33" r="30"></circle>
        </svg>
    </div>
</template>
<script src="{{ 'bootstrap.js'|asset_path }}"></script>
<script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-72513973-1', 'auto');
    ga('send', 'pageview');

</script>
</body>
</html>
