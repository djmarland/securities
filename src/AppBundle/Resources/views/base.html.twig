    {#<link rel="icon" type="image/png" href="/static/icons/android-chrome-192x192.png" sizes="192x192" />#}
    {#<link rel="icon" type="image/png" href="/static/icons/favicon-16x16.png" sizes="16x16" />#}
    {#<meta name="msapplication-TileColor" content="#383b3f" />#}
    {#<meta name="msapplication-TileImage" content="/static/icons/mstile-144x144.png" />#}
    {#<meta name="msapplication-square150x150logo" content="/static/icons/mstile-150x150.png" />#}
    {#<meta name="msapplication-square310x310logo" content="/static/icons/mstile-310x310.png" />#}
    {#<meta name="theme-color" content="#383b3f" />#}
    <link rel="stylesheet" href="{{ 'global.css'|asset_path }}" />
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script>
        google.charts.load('43', {'packages':['corechart']});
    </script>
<body class="{% block bodyClass %}{% endblock %}" data-env="{{ meta.environment }}">
{% block header %}
<div class="app">
    <header><div class="header">
            <div class="page-limit">
                <div id="masthead-content" class="header__content">
                <div class="header__logo">
                    <a href="{{ path('home') }}">{{ meta.siteTitle }}</a>
                </div>
                <div id="search-container" class="header__search">{% include '@partials/search.html.twig' %}</div>
                </div>
            </div>
    </div></header>
    <nav><div class="nav">
            <ul class="nav__list page-limit"
                ><li class="nav__item"><a class="nav__link {% if currentSection == 'securities' %}nav__link--active{% endif %}" href="{{ path('security_list') }}">Securities</a></li
                ><li class="nav__item"><a class="nav__link {% if currentSection == 'issuers' %}nav__link--active{% endif %}" href="{{ path('issuer_list') }}">Issuers</a></li
                >{#<li class="nav__item"><a class="nav__link {% if currentSection == 'industries' %}nav__link--active{% endif %}" href="{{ path('industry_list') }}">Industries</a></li
                ><li class="nav__item"><a class="nav__link {% if currentSection == 'about' %}nav__link--active{% endif %}" href="{{ path('about') }}">About</a></li
            >#}</ul>
    </div></nav>
    <main><div class="main">
        <div class="page-limit" id="main-body">
{% endblock %}
{% block body %}{% endblock %}
{% block footer %}
        </div>
    </div></main>
    <footer><div class="footer">
        <div class="page-limit">
            &copy; {{ currentYear }} ISIN Analytics Ltd | <a href="{{ path('terms') }}">Terms of use</a> | <a href="{{ path('privacy_policy') }}">Privacy policy</a>
        </div>
    </div></footer>
</div>
{% endblock %}
<template id="loading-spinner" class="hidden">
    <div class="loading">
        <svg class="loading__spinner" viewBox="-2 -2 70 70" xmlns="http://www.w3.org/2000/svg">
            <circle class="loading__path" fill="none" stroke-width="8" cx="33" cy="33" r="30"></circle>
        </svg>
    </div>
</template>
<script src="{{ 'bootstrap.js'|asset_path }}"></script>
<!-- Begin Cookie Consent plugin by Silktide - http://silktide.com/cookieconsent -->
<script type="text/javascript">
    window.cookieconsent_options = {
        "message":"This website uses cookies to ensure you get the best experience on our website",
        "dismiss":"OK",
        "learnMore":"More info",
        "link":"/privacy-policy",
        "theme":"dark-bottom"
    };
</script>
<script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/cookieconsent2/1.0.9/cookieconsent.min.js"></script>
<!-- End Cookie Consent plugin -->
<script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-72513973-1', 'auto');
    ga('send', 'pageview');

</script>
{% if adverts.areActive %}
    <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
    <script>(adsbygoogle = window.adsbygoogle || []).push({});</script>
{% endif %}
</body>
</html>
