<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>{{ meta.fullTitle }}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="apple-touch-icon" sizes="57x57" href="/apple-touch-icon-57x57.png">
    <link rel="apple-touch-icon" sizes="60x60" href="/apple-touch-icon-60x60.png">
    <link rel="apple-touch-icon" sizes="72x72" href="/apple-touch-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="76x76" href="/apple-touch-icon-76x76.png">
    <link rel="apple-touch-icon" sizes="114x114" href="/apple-touch-icon-114x114.png">
    <link rel="apple-touch-icon" sizes="120x120" href="/apple-touch-icon-120x120.png">
    <link rel="apple-touch-icon" sizes="144x144" href="/apple-touch-icon-144x144.png">
    <link rel="apple-touch-icon" sizes="152x152" href="/apple-touch-icon-152x152.png">
    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon-180x180.png">
    <link rel="icon" type="image/png" href="/favicon-32x32.png" sizes="32x32">
    <link rel="icon" type="image/png" href="/favicon-194x194.png" sizes="194x194">
    <link rel="icon" type="image/png" href="/favicon-96x96.png" sizes="96x96">
    <link rel="icon" type="image/png" href="/android-chrome-192x192.png" sizes="192x192">
    <link rel="icon" type="image/png" href="/favicon-16x16.png" sizes="16x16">
    <link rel="manifest" href="/manifest.json">
    <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#342638">
    <meta name="apple-mobile-web-app-title" content="{{ meta.siteTitle }}">
    <meta name="application-name" content="{{ meta.siteTitle }}">
    <meta name="msapplication-TileColor" content="#342638">
    <meta name="msapplication-TileImage" content="/mstile-144x144.png">
    <meta name="theme-color" content="#342638">

    <meta property="og:site_name" content="{{ meta.siteTitle }}" />
    <meta property="og:title" content="{{ meta.fullTitle }}" />
    <meta property="og:url" content="{{ meta.canonicalUrl }}" />
    <meta property="og:type" content="website" />
    <meta property="og:description" content="The Gateway to London's Debt Capital Markets" />
    <meta property="og:image" content="{{ appConfig.siteHostName }}{{ 'fb-cover.png'|asset_path }}"/>

    <meta name="twitter:card" content="summary" />
    <link rel="image_src" href="{{ appConfig.siteHostName }}{{ 'fb-cover.png'|asset_path }}" />

    <link rel="canonical" href="{{ meta.canonicalUrl }}" />

    {% if pagination %}
        {% if pagination.prevUrl %}
            <link rel="prev" href="{{ pagination.prevUrl }}" />
        {% endif %}
        {% if pagination.nextUrl %}
            <link rel="next" href="{{ pagination.nextUrl }}" />
        {% endif %}
    {% endif %}
    <link href='https://fonts.googleapis.com/css?family=Source+Sans+Pro:400,600' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="{{ 'global.css'|asset_path }}" />
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script>
        google.charts.load('43', {'packages':['corechart']});
    </script>
<body class="{% block bodyClass %}{% endblock %}" data-env="{{ meta.environment }}">
{% block header %}
<div class="app">
    <header><div class="header">
            <div class="page-limit">
                <div id="masthead-content" class="header__content">
                <div class="header__logo">
                    <a href="{{ path('home') }}">
                        <img src="{{ 'logo.svg'|asset_path }}" alt="{{ meta.siteTitle }}"/>
                    </a>
                </div>
                <div id="search-container" class="header__search">{% include '@partials/search.html.twig' %}</div>
                </div>
            </div>
    </div></header>
    <nav><div class="nav">
            <ul class="nav__list page-limit"
                ><li class="nav__item"><a class="nav__link {% if currentSection == 'securities' %}nav__link--active{% endif %}" href="{{ path('overview_securities') }}">Securities</a></li
                ><li class="nav__item"><a class="nav__link {% if currentSection == 'issuers' %}nav__link--active{% endif %}" href="{{ path('issuer_list') }}">Issuers</a></li
                >{#<li class="nav__item"><a class="nav__link {% if currentSection == 'industries' %}nav__link--active{% endif %}" href="{{ path('industry_list') }}">Industries</a></li
                ><li class="nav__item"><a class="nav__link {% if currentSection == 'about' %}nav__link--active{% endif %}" href="{{ path('about') }}">About</a></li
            >#}</ul>
    </div></nav>
    <main><div class="main">
        <div class="page-limit" id="main-body">
{% endblock %}
{% block body %}{% endblock %}
{% block footer %}
        </div>
    </div></main>
    <footer><div class="footer">
        <div class="page-limit">
            &copy; {{ currentYear }} ISIN Analytics Ltd | <a href="{{ path('terms') }}">Terms of use</a> | <a href="{{ path('privacy_policy') }}">Privacy policy</a>
        </div>
    </div></footer>
</div>
{% endblock %}
<template id="loading-spinner" class="hidden">
    <div class="loading">
        <svg class="loading__spinner" viewBox="-2 -2 70 70" xmlns="http://www.w3.org/2000/svg">
            <circle class="loading__path" fill="none" stroke-width="8" cx="33" cy="33" r="30"></circle>
        </svg>
    </div>
</template>
{#<script src="{{ 'bootstrap.js'|asset_path }}"></script>#}
<!-- Begin Cookie Consent plugin by Silktide - http://silktide.com/cookieconsent -->
<script type="text/javascript">
    window.cookieconsent_options = {
        "message":"This website uses cookies to ensure you get the best experience on our website",
        "dismiss":"OK",
        "learnMore":"More info",
        "link":"/privacy-policy",
        "theme":"dark-bottom"
    };
</script>
<script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/cookieconsent2/1.0.9/cookieconsent.min.js"></script>
<!-- End Cookie Consent plugin -->
{% if adverts.areActive %}
    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

        ga('create', 'UA-72513973-1', 'auto');
        ga('send', 'pageview');

    </script>
    <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
    <script>(adsbygoogle = window.adsbygoogle || []).push({});</script>
{% endif %}
</body>
</html>
